# EmailJS Configuration
# Copy this file to .env and replace with your actual credentials
# Get your credentials from: https://dashboard.emailjs.com/

# Service ID - from your EmailJS dashboard (e.g., service_abc123)
REACT_APP_EMAILJS_SERVICE_ID=your_service_id_here

# Template ID - from your EmailJS email templates (e.g., template_xyz789)
REACT_APP_EMAILJS_TEMPLATE_ID=your_template_id_here

# Public Key (User ID) - from your EmailJS account settings (e.g., user_AbCdEfGh)
REACT_APP_EMAILJS_PUBLIC_KEY=your_public_key_here

# Optional: Enable test mode (set to 'true' to test without sending emails)
# REACT_APP_TEST_MODE=false
